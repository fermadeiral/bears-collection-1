<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <ui:with field='constants' type='org.ovirt.engine.ui.common.CommonApplicationConstants'/>

    <g:FlowPanel>
        <b:Row>
            <b:Column size="SM_6">
                <ge:StringEntityModelTextBoxEditor ui:field="nameEditor" label="{constants.nameNetworkFilterParameter}" usePatternFly="true" removeFormGroup="true"/>
            </b:Column>
            <b:Column size="SM_6">
                <ge:StringEntityModelTextBoxEditor ui:field="valueEditor" label="{constants.valueNetworkFilterParameter}" usePatternFly="true" removeFormGroup="true"/>
            </b:Column>
        </b:Row>
    </g:FlowPanel>
</ui:UiBinder>
